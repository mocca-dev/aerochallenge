{
  "version": 2,
  "name": "aerochallenge",
  "public": true,
  "builds": [
    { "src": "package.json", "use": "@now/static-build" },
    { "src": "/api/app.js", "use": "@now/node-server" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/app.js" },
    { "src": "^/static/(.*)", "dest": "/static/$1" },
    { "src": "^/assets/(.*)", "dest": "/assets/$2" },
    { "src": ".*", "dest": "/index.html" }
  ]
}
